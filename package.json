{
  "name": "main-service",
  "version": "0.0.1",
  "description": "nestjs app main service",
  "author": "dwi@nvpdev.tech",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "typeorm": "tsx ./node_modules/typeorm/cli",
    "gen": "plop",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "npm run typeorm -- --dataSource=src/db/_cli.db.ts migration:generate ./src/db/migrations/$npm_config_name",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- --dataSource=src/db/_cli.db.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=src/db/_cli.db.ts migration:revert",
    "migration:pending": "tsx ./src/scripts/pending-migrations.ts",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/db/seeds/seed.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.0.5",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.5",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.5",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/serve-static": "^5.0.1",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.11",
    "aws-sdk": "^2.1693.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.66.4",
    "cache-manager": "^7.2.7",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^8.0.1",
    "dotenv": "^16.4.7",
    "express-session": "^1.18.1",
    "fast-csv": "^5.0.5",
    "helmet": "^8.0.0",
    "ioredis": "^5.9.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nestjs-pino": "^4.3.0",
    "nodemailer": "^6.10.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.3",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.5",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager": "^5.0.0",
    "@types/connect-redis": "^0.0.23",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/multer": "^1.4.13",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "22.10.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "plop": "^4.0.1",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/$1",
      "^@mod/(.*)$": "<rootDir>/modules/$1"
    }
  }
}
